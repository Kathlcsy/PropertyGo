{
  "name": "PropertyGo Dev Container",
  "services": {
    "mysql": {
      "image": "mysql:8.0",
      "env": {
        "MYSQL_ROOT_PASSWORD": "rootpassword",
        "MYSQL_DATABASE": "propertygo",
        "MYSQL_USER": "user",
        "MYSQL_PASSWORD": "password"
      },
      "ports": ["3306"]
    }
  },
  "forwardPorts": [3000, 3306],
  "postCreateCommand": "npm install",
  "extensions": ["dbaeumer.vscode-eslint"]
}
